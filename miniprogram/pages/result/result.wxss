/* miniprogram/pages/result/result.wxss */
.title{
  width: 100%;
  height: 40px;
  line-height: 40px;
  border-top: 1px solid rgb(112, 160, 189);
  background: #2B5565;
  color: white;
  padding-left: 10px;
  font-size: 14px;
}

.resultGet{
  width: 90%;
  border: 1px solid #267CC1;
  margin: 5px;
  border-radius: 2px;
  height: 60px;
  border-radius: 5px;
  background-color: #267CC1;
  margin: auto;
  margin-top: 10px;
}

.nameValue,.locationValue{
  font-size: 13px;
  margin-left: 10px;
  height: 24px;
  line-height: 24px;
  height: 30px;
  color: white;
}
